      "source": [
        "import matplotlib.pyplot as plt\n",
        "import math\n",
        "# Edit these parameters to try different loss measurements. Rerun this cell when\n",
        "# done. Your Y will be calculated as Y=wX+b, so if w=3, and b=-1, then Y=3x-1.\n",
        "\n",
        "\n",
        "w = 3 # slope\n",
        "b = -1 # y-intercept\n",
        "\n",
        "x = [-1, 0, 1, 2, 3, 4]\n",
        "y = [-3, -1, 1, 3, 5, 7]\n",
        "myY = []\n",
        "\n",
        "\n",
        "for thisX in x:\n",
        "  thisY = (w*thisX)+b\n",
        "  myY.append(thisY)\n",
        "  #print(x.index(thisX)) #printing the index of each list item\n",
        "\n",
        "print(f\"Real Y is {str(y)}\")\n",
        "print(f\"My Y is   {str(myY)}\")\n",
        "\n",
        "# let's calculate the loss\n",
        "total_square_error = 0\n",
        "for i in range(0, len(y)):\n",
        "  ## printing the index\n",
        "  #print(i)\n",
        "\n",
        "  ## I am plotting the residuals here\n",
        "  point1 = [x[i],x[i]]\n",
        "  point2 = [y[i],myY[i]]\n",
        "  #print(point1, '||', point2)\n",
        "  plt.plot(point1, point2, color='red')\n",
        "\n",
        "  ## The total loss is being calculated here\n",
        "  square_error = (y[i] - myY[i]) ** 2\n",
        "  total_square_error += square_error\n",
        "\n",
        "## I am printing the residuals\n",
        "#print(residuals)\n",
        "\n",
        "## I am printing my total loss\n",
        "print(f\"My loss is: {str(math.sqrt(total_square_error))}\")\n",
        "\n",
        "## I am plotting the chart which displays the linear regression\n",
        "plt.plot(x, y)\n",
        "plt.plot(x, y, 'o')\n",
        "plt.plot(x, myY, 'o')\n",
        "plt.title('Linear Regression')\n",
        "plt.xlabel(\"Independent Variable (x)\", fontsize = 15)\n",
        "plt.ylabel(\"Dependent Variable (y)\",fontsize = 15)"
      ]
